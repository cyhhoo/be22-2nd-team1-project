# 개발 환경 민감 정보 예제
# 이 파일을 복사해서 application-dev.yml 생성 후 실제 값으로 변경하세요

spring:
  datasource:
    url: jdbc:mariadb://localhost:3306/schoolmate
    username: YOUR_DB_USERNAME  # 실제 DB 사용자명으로 변경
    password: YOUR_DB_PASSWORD  # 실제 DB 비밀번호로 변경

jwt:
  secret: YOUR_JWT_SECRET_KEY_AT_LEAST_256_BITS  # 최소 256비트 이상의 시크릿 키
